{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^4",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3",
        "easycorp/easyadmin-bundle": "^4.10",
        "league/flysystem-bundle": "^3.3",
        "nelmio/cors-bundle": "^2.2",
        "nesbot/carbon": "^3.3",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^1.16 | ^2",
        "runtime/frankenphp-symfony": "^0.2",
        "sentry/sentry-symfony": "^5.0",
        "symfony/asset": "^7",
        "symfony/asset-mapper": "^7",
        "symfony/console": "^7",
        "symfony/doctrine-messenger": "^7",
        "symfony/dotenv": "^7",
        "symfony/expression-language": "^7",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "^7",
        "symfony/http-client": "^7",
        "symfony/mercure-bundle": "^0.3.5",
        "symfony/messenger": "^7",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "^7",
        "symfony/property-info": "^7",
        "symfony/runtime": "^7",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^7",
        "symfony/stimulus-bundle": "^2.17",
        "symfony/twig-bundle": "^7",
        "symfony/uid": "^7",
        "symfony/ux-live-component": "^2.17",
        "symfony/validator": "^7",
        "symfony/yaml": "^7",
        "symfonycasts/dynamic-forms": "^0.1.2",
        "symfonycasts/micro-mapper": "^0.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^2.5"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.57",
        "league/flysystem-memory": "^3.29",
        "mockery/mockery": "^1.6",
        "mtdowling/jmespath.php": "^2.7",
        "seld/jsonlint": "^1.10",
        "symfony/browser-kit": "^7",
        "symfony/css-selector": "^7",
        "symfony/debug-bundle": "^7",
        "symfony/maker-bundle": "^1.59",
        "symfony/phpunit-bridge": "^7",
        "symfony/stopwatch": "^7",
        "symfony/var-dumper": "^7",
        "symfony/web-profiler-bundle": "^7",
        "zenstruck/browser": "^1.8",
        "zenstruck/foundry": "^2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7",
            "docker": false
        }
    }
}
